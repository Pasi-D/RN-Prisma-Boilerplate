type User {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  name: String
  email: String! @unique
  password: String!
  products: [Product!]!
}

type Product {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  name: String!
  pictrueUrl: String!
  price: Float!
  seller: User!
}
